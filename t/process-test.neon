IMPORT process

VAR out, err: Bytes
VAR r: Number

r := process.call("bin/neon", OUT out, OUT err)
ASSERT r = 1
ASSERT out = HEXBYTES ""
ASSERT err.decodeToString()[0 TO 28] = "Usage: bin/neon filename.neon"

r := process.call("bin/neon t/hello.neon", OUT out, OUT err)
ASSERT r = 0
ASSERT out.decodeToString()[0 TO 10] = "hello world"
ASSERT err = HEXBYTES ""
