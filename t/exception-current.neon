EXCEPTION PrinterOnFireException

TRY
    RAISE PrinterOnFireException(info WITH "A4")
TRAP PrinterOnFireException DO
    print(CURRENT_EXCEPTION.name)
    print(CURRENT_EXCEPTION.info)
    print(str(CURRENT_EXCEPTION.offset))
END TRY
print(CURRENT_EXCEPTION.name)

