FUNCTION f_jf(INOUT self: Executor)
    INC self.ip
    LET target: Number := getVint(self.bytecode.code, INOUT self.ip)
    LET b: Boolean := self.pop()->b
    IF NOT b THEN
        self.ip := target
    END IF
END FUNCTION

