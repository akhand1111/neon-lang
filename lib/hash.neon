 |%

%|
 |  Function: crc32
 |
 |  Return the CRC32 of a given <String>.
 |%
DECLARE NATIVE FUNCTION crc32(data: String): Number

%|
 |  Function: crc32_bytes
 |
 |  Return the CRC32 of given <Bytes>.
 |%
DECLARE NATIVE FUNCTION crc32_bytes(data: Bytes): Number

